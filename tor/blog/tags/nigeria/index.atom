	<title>Dear Nigerians, help us help you.</title>

	<id>http://localhost/jmtodaro/tor/blog/posts/Dear_Nigerians_help_us_help_you_/</id>

	<link href="http://localhost/jmtodaro/tor/blog/posts/Dear_Nigerians_help_us_help_you_/"/>

	<author><name>phobos</name></author>






	<category term="/tags/data_gathering" />

	<category term="/tags/herdict" />

	<category term="/tags/nigeria" />

	<category term="/tags/support" />

	<category term="/tags/tor_blocked" />

