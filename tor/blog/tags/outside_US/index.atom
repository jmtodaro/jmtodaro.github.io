	<title>Thanks to Shinjiru</title>

	<id>http://localhost/jmtodaro/tor/blog/posts/Thanks_to_Shinjiru/</id>

	<link href="http://localhost/jmtodaro/tor/blog/posts/Thanks_to_Shinjiru/"/>

	<author><name>phobos</name></author>






	<category term="/tags/directory_authority" />

	<category term="/tags/donated_services" />

	<category term="/tags/outside_EU" />

	<category term="/tags/outside_US" />


	<updated>2011-04-05T11:43:25Z</updated>
