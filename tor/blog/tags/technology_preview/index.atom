	<title>Technology Preview:  Marble and Vidalia-0.2.0</title>

	<id>http://localhost/jmtodaro/tor/blog/posts/Technology_Preview__Marble_and_Vidalia-0_2_0/</id>

	<link href="http://localhost/jmtodaro/tor/blog/posts/Technology_Preview__Marble_and_Vidalia-0_2_0/"/>

	<author><name>phobos</name></author>






	<category term="/tags/Qt_updates" />

	<category term="/tags/drag_and_drop_OS_X_install" />

	<category term="/tags/experimental" />

	<category term="/tags/kde_marble" />

	<category term="/tags/msi_installer" />

	<category term="/tags/technology_preview" />

