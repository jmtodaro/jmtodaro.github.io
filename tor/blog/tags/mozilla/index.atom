<entry>
	<title>Torbutton Release 1.2.5, Google Captchas, and addons.mozilla.org</title>

	<id>http://localhost/jmtodaro/tor/blog/posts/Torbutton_Release_1_2_5_Google_Captchas_and_addons_mozilla_org/</id>

	<link href="http://localhost/jmtodaro/tor/blog/posts/Torbutton_Release_1_2_5_Google_Captchas_and_addons_mozilla_org/"/>

	<author><name>mikeperry</name></author>






	<category term="/tags/captchas" />

	<category term="/tags/data_retention" />

	<category term="/tags/firefox" />

	<category term="/tags/google" />

	<category term="/tags/logging" />

	<category term="/tags/mozilla" />

