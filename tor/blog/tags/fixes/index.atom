	<title>November 2010 Progress Report</title>

	<id>http://localhost/jmtodaro/tor/blog/posts/November_2010_Progress_Report/</id>

	<link href="http://localhost/jmtodaro/tor/blog/posts/November_2010_Progress_Report/"/>

	<author><name>phobos</name></author>






	<category term="/tags/enhancements" />

	<category term="/tags/fixes" />

